{
  "name": "govcms8_uikit_starter",
  "version": "1.0.0",
  "description": "Starter theme for GovCMS 8 integrating UI Kit.",
  "main": "gulpfile.js",
  "author": "morpht.com",
  "scripts": {
    "setup": "npm ci",
    "styleguide": "kss --config kss-config.json",
    "gulp": "gulp",
    "gulp dev": "gulp dev",
    "js-lint": "gulp js-lint"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "browser-sync": "^2.26.7",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^6.2.0",
    "gulp-jshint": "^2.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "jshint": "^2.11.0",
    "kss": "^3.0.0",
    "node-sass": "^4.13.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "ie 10"
  ],
  "dependencies": {
    "@gov.au/accordion": "^7.0.8",
    "@gov.au/animate": "^1.0.13",
    "@gov.au/body": "^2.2.0",
    "@gov.au/breadcrumbs": "^3.0.5",
    "@gov.au/buttons": "^3.0.7",
    "@gov.au/callout": "^3.0.2",
    "@gov.au/card": "^0.3.1",
    "@gov.au/control-input": "^3.0.1",
    "@gov.au/core": "^3.4.2",
    "@gov.au/cta-link": "^2.2.0",
    "@gov.au/direction-links": "^3.0.0",
    "@gov.au/footer": "^3.0.5",
    "@gov.au/form": "^0.1.4",
    "@gov.au/grid-12": "^2.1.0",
    "@gov.au/header": "^4.1.12",
    "@gov.au/headings": "^2.0.11",
    "@gov.au/inpage-nav": "^3.0.6",
    "@gov.au/keyword-list": "^3.0.5",
    "@gov.au/link-list": "^3.0.8",
    "@gov.au/main-nav": "^1.0.7",
    "@gov.au/page-alerts": "^2.1.1",
    "@gov.au/pancake": "^1.4.1",
    "@gov.au/pancake-js": "^1.2.1",
    "@gov.au/pancake-json": "^1.2.0",
    "@gov.au/pancake-react": "^1.3.0",
    "@gov.au/pancake-sass": "^2.3.7",
    "@gov.au/progress-indicator": "^3.1.7",
    "@gov.au/responsive-media": "^2.0.14",
    "@gov.au/select": "^2.0.11",
    "@gov.au/side-nav": "^5.0.7",
    "@gov.au/skip-link": "^2.0.16",
    "@gov.au/table": "^0.2.0",
    "@gov.au/tags": "^4.0.0",
    "@gov.au/text-inputs": "^2.1.1"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "json": {
      "enable": false,
      "location": "assets/uikit",
      "name": "uikit",
      "content": {
        "name": true,
        "version": true,
        "dependencies": true,
        "path": true,
        "settings": true
      }
    },
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 10"
      ],
      "location": "assets/uikit/css/",
      "name": "uikit.min.css"
    },
    "sass": {
      "modules": false,
      "location": "assets/uikit/sass/",
      "name": "uikit.scss"
    },
    "js": {
      "minified": true,
      "modules": true,
      "location": "assets/uikit/js/",
      "name": "uikit.min.js"
    },
    "react": {
      "location": "assets/uikit/react/"
    }
  }
}