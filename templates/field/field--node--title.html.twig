{%- for item in items -%}
  {% set pageTitleTemplate = directory ~ '/templates/content/page-title.html.twig' %}
  {% set defaultPageTitleTemplate = '@govcms8_uikit_starter/content/page-title.html.twig' %}
  {%
    include [pageTitleTemplate, defaultPageTitleTemplate] with {
      title: item.content,
      title_attributes: attributes
    } only
  %}
{%- endfor -%}
